{
  "name": "@sk-web-gui/web-app-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "node middleware-envs-generator.mjs",
    "prebuild": "node middleware-envs-generator.mjs",
    "dev": "cross-env TEST=true next dev",
    "build": "next build",
    "build:test": "cross-env TEST=true next build",
    "start": "next start",
    "generate:contracts": "cross-env NODE_ENV=development npx tsx ./src/swagger-typescript-api.ts",
    "lint": "next lint --no-cache",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "cypress": "cypress open",
    "cypress:headless": "cross-env TEST=true npx cypress run --component && npx cypress run --e2e",
    "jest": "jest --watch",
    "jest:coverage": "jest --coverage",
    "test": "concurrently \"yarn jest\" \"yarn cypress\"",
    "coverage": "npx nyc report --reporter=text-summary",
    "test:coverage": "yarn cypress:headless",
    "type-check": "tsc --pretty --noEmit"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.0",
    "@next/bundle-analyzer": "^15.2.4",
    "@sk-web-gui/core": "3.11.2",
    "@sk-web-gui/react": "1.4.1",
    "@tailwindcss/forms": "^0.5.10",
    "axios": "^1.8.4",
    "dayjs": "^1.11.13",
    "i18next": "^24.2.3",
    "i18next-resources-to-backend": "^1.2.1",
    "lucide-react": "^0.471.1",
    "next": "15.2.4",
    "next-i18n-router": "^5.5.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.55.0",
    "react-i18next": "^15.4.1",
    "sass": "^1.86.1",
    "sharp": "^0.33.5",
    "swc-plugin-coverage-instrument": "^0.0.26",
    "underscore.string": "^3.3.6",
    "yup": "^1.6.1",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@babel/plugin-transform-private-methods": "^7.25.9",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@cypress/code-coverage": "^3.14.0",
    "@eslint/js": "^8.57.1",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "22.13.17",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.1",
    "@types/underscore.string": "^0.0.41",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "autoprefixer": "^10.4.21",
    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "cypress": "^14.2.1",
    "dotenv": "^16.4.7",
    "envalid": "^8.0.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.2.4",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.5.3",
    "postcss-import": "^16.1.0",
    "postcss-nested": "^7.0.2",
    "postcss-nesting": "^13.0.1",
    "prettier": "^3.5.3",
    "swagger-typescript-api": "^13.0.28",
    "tailwindcss": "^3.4.17",
    "typescript": "5.8.2",
    "typescript-eslint": "^8.29.0"
  },
  "browserslist": [
    "defaults and fully supports es6-module"
  ]
